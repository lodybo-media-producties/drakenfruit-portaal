{
  "name": "drakenfruit-portaal",
  "version": "0.1.1",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "postinstall": "node ./postinstall.js",
    "build": "run-s build:*",
    "build:remix": "remix build",
    "build:server": "esbuild --platform=node --format=cjs ./server.ts --outdir=build --bundle --external:fsevents",
    "build:storybook": "npm run storybook:build",
    "dev": "run-p dev:*",
    "dev:remix": "remix dev --manual -c \"node --require ./mocks ./build/server.js\"",
    "dev:storybook": "npm run storybook",
    "dev:db-studio": "npm run db:studio",
    "docker:start": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": "prisma generate && prisma migrate deploy && prisma db seed",
    "start": "cross-env NODE_ENV=production node ./build/server.js",
    "start:mocks": "cross-env NODE_ENV=production node --require ./mocks --require dotenv/config ./build/server.js",
    "test": "vitest",
    "test:dev": "run-p dev:remix",
    "test:build": "run-s build:remix build:server",
    "test:e2e:dev": "start-server-and-test test:dev http://localhost:3000 \"npx cypress open\"",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "cross-env PORT=8811 start-server-and-test start:mocks http://localhost:8811 \"npx cypress run\"",
    "typecheck": "tsc && tsc -p cypress",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push && prisma generate",
    "db:seed": "prisma db seed",
    "db:reset": "prisma migrate reset",
    "db:studio": "prisma studio --browser none",
    "storybook": "storybook dev -p 6006 --no-open",
    "storybook:build": "storybook build",
    "sui": "shadcn-ui",
    "sui:add": "shadcn-ui add -y",
    "sui:diff": "shadcn-ui diff",
    "generate": "plop component --"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build",
    "/public/tinymce/",
    "/postgres-data",
    "/storybook-static"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-brands-svg-icons": "^6.4.2",
    "@fortawesome/free-regular-svg-icons": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@isaacs/express-prometheus-middleware": "^1.2.1",
    "@prisma/client": "^5.3.1",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.5",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@remix-run/css-bundle": "^2.0.0",
    "@remix-run/express": "^2.0.0",
    "@remix-run/node": "^2.0.0",
    "@remix-run/react": "^2.0.0",
    "@remix-run/serve": "^2.0.0",
    "@remix-run/server-runtime": "^2.0.0",
    "@sindresorhus/slugify": "^2.2.1",
    "@tinymce/tinymce-react": "^4.3.0",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cmdk": "^0.2.0",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "i18next": "^23.5.1",
    "i18next-browser-languagedetector": "^7.1.0",
    "i18next-fs-backend": "^2.2.0",
    "i18next-http-backend": "^2.2.2",
    "isbot": "^3.7.0",
    "lucide-react": "^0.279.0",
    "morgan": "^1.10.0",
    "prisma": "^5.3.1",
    "prisma-json-types-generator": "^3.0.1",
    "prom-client": "^14.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.2.2",
    "remix-i18next": "^5.4.0",
    "remix-utils": "^7.0.0-pre.3",
    "sharp": "^0.32.6",
    "source-map-support": "^0.5.21",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.7",
    "tiny-invariant": "^1.3.1",
    "tinymce": "^6.5.1",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.1.0",
    "@remix-run/dev": "^2.0.0",
    "@remix-run/eslint-config": "^2.0.0",
    "@remix-run/testing": "^2.0.1",
    "@storybook/addon-a11y": "^7.4.5",
    "@storybook/addon-essentials": "^7.4.5",
    "@storybook/addon-interactions": "^7.4.5",
    "@storybook/addon-links": "^7.4.5",
    "@storybook/addon-styling": "^1.3.7",
    "@storybook/blocks": "^7.4.5",
    "@storybook/react": "^7.4.5",
    "@storybook/react-vite": "^7.4.5",
    "@storybook/testing-library": "^0.2.1",
    "@testing-library/cypress": "^10.0.1",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/bcryptjs": "^2.4.4",
    "@types/compression": "^1.7.3",
    "@types/eslint": "^8.44.2",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.5",
    "@types/node": "^20.6.3",
    "@types/react": "^18.2.22",
    "@types/react-dom": "^18.2.7",
    "@types/source-map-support": "^0.5.7",
    "@vitejs/plugin-react": "^4.0.4",
    "@vitest/coverage-v8": "^0.34.4",
    "autoprefixer": "^10.4.15",
    "c8": "^8.0.1",
    "cookie": "^0.5.0",
    "cypress": "^13.2.0",
    "dotenv": "^16.3.1",
    "esbuild": "^0.19.3",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-storybook": "^0.6.14",
    "happy-dom": "^12.1.2",
    "msw": "^1.3.1",
    "npm-run-all": "^4.1.5",
    "plop": "^4.0.0",
    "postcss": "^8.4.30",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "prop-types": "^15.8.1",
    "shadcn-ui": "^0.4.0",
    "start-server-and-test": "^2.0.0",
    "storybook": "^7.4.5",
    "storybook-addon-deep-controls": "^0.2.1",
    "storybook-react-i18next": "^2.0.9",
    "tailwindcss": "^3.3.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "vite": "^4.4.9",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.4"
  },
  "engines": {
    "node": ">=14"
  },
  "prisma": {
    "seed": "ts-node --require tsconfig-paths/register prisma/seed.ts"
  }
}
